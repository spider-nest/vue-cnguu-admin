{
  "name": "vue-spider-admin",
  "version": "0.0.1",
  "description": "Backend templates for vue",
  "keywords": [
    "vue",
    "vue3",
    "ant-design",
    "ant-design-vue",
    "ant-design-vue-pro",
    "ant-design-vue-admin"
  ],
  "author": {
    "name": "cnguu",
    "email": "www@cnguu.cn",
    "url": "https://github.com/cnguu"
  },
  "repository": "https://github.com/spider-nest/vue-spider-admin.git",
  "homepage": "https://github.com/spider-nest/vue-spider-admin",
  "bugs": {
    "url": "https://github.com/spider-nest/vue-spider-admin/issues"
  },
  "license": "MIT",
  "main": "./src/main.ts",
  "scripts": {
    "serve": "cross-env --max_old_space_size=4096 vite",
    "build": "vite build && esno ./build/bootstrap.ts",
    "build:icon": "esno ./build/build-icon.ts",
    "report": "cross-env REPORT=true yarn build",
    "preview": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "clear:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clear:modules": "rimraf node_modules",
    "clear:install": "rimraf yarn.lock && rimraf node_modules && yarn",
    "install:husky": "is-ci || husky install",
    "postinstall": "yarn install:husky"
  },
  "dependencies": {
    "@iconify/iconify": "^2.0.0-rc.6",
    "@vueuse/core": "^4.8.2",
    "ant-design-vue": "^2.1.2",
    "axios": "^0.21.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-alpha.13",
    "vue": "^3.0.11",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.330",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.41",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.3",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-define-config": "^1.0.7",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "esno": "^0.5.0",
    "fs-extra": "^9.1.0",
    "husky": "^6.0.0",
    "inquirer": "^8.0.0",
    "is-ci": "^3.0.0",
    "less": "^4.1.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup-plugin-visualizer": "^5.4.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.2.4",
    "vite": "^2.1.5",
    "vite-plugin-compression": "^0.2.4",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-imagemin": "^0.3.0",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-style-import": "^0.9.2",
    "vite-plugin-svg-icons": "^0.4.3",
    "vite-plugin-theme": "^0.7.1",
    "vue-eslint-parser": "^7.6.0"
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
